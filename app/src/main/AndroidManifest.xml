<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.android.musictouch">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".NowPlaying" android:label="@string/now_playing" />
        <activity android:name=".Artists" android:label="@string/artists"/>
        <activity android:name=".Albums" android:label="@string/albums"/>
        <activity android:name=".AllSongs" android:label="@string/all_songs"/>
        <activity android:name=".DiscoverNewSongs" android:label="@string/discover" />
        <activity android:name=".PaymentActivity" android:label="@string/payment_label"/>
    </application>

</manifest>